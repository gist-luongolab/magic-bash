#Alias cd
function cd() { 
	command cd $1
	ls -G 
}
function cdh() { 
	cd ${HOME}
	ls -G 
}

#erase extensions file
function dext(){
	for i in  $(ls -1 *.$1); do mv "$i" "$(echo $i |sed 's/\(.*\)\..*/\1/')";done
}
